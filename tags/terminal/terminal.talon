os: mac
tag: terminal
-
# Editing text
clear word:
    key(alt-right)
    key(alt-backspace)

clear word right:
    key(alt-delete)

clear word left:
    key(alt-backspace)

chop line: key(ctrl-k)
paste chop: key(ctrl-y)

# Process management
kill this: key(ctrl-c)

suspend: key(ctrl-z)

foreground:
    key(f)
    key(g)
    key(enter)