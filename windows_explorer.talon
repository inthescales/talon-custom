app: windows explorer
-
# Window
# go parent: key(alt-up)
go (address | path): key(alt-d)
go search: key(ctrl-e)

copy (address | path):
	key(alt-d)
	key(ctrl-c)

# New objects
new folder: key(ctrl-shift-n)
new text:
	key(shift-f10)
	key(w)
	key(t)

# File commands
properties: key(alt-enter)
rename: key(f2)
delete: key(delete)
